<h1>New template</h1>

<% form_for(@template) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :namestyle %><br />
    <%= f.text_field :namestyle %>
  </p>
  <p>
    <%= f.label :contentfile %><br />
    <%= f.text_field :contentfile %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', templates_path %>
